<?xml version="1.0" encoding="UTF-8"?>
<web-app  version="3.1"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
         
    <servlet>
      <servlet-name>Browse Servlet</servlet-name>
      <servlet-class>ua.nure.kn156.khorshunova.web.BrowseServlet</servlet-class>
    </servlet>
    
    <servlet>
      <servlet-name>Edit Servlet</servlet-name>
      <servlet-class>ua.nure.kn156.khorshunova.web.EditServlet</servlet-class>
    </servlet>
    
    <servlet>
      <servlet-name>Add Servlet</servlet-name>
      <servlet-class>ua.nure.kn156.khorshunova.web.AddServlet</servlet-class>
    </servlet>
    
    <servlet>
      <servlet-name>Details Servlet</servlet-name>
      <servlet-class>ua.nure.kn156.khorshunova.web.DetailsServlet</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>Browse Servlet</servlet-name>
        <url-pattern>/browse</url-pattern>
    </servlet-mapping>
       
    <servlet-mapping>
        <servlet-name>Edit Servlet</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
       
    <servlet-mapping>
        <servlet-name>Add Servlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
         
    <servlet-mapping>
        <servlet-name>Details Servlet</servlet-name>
        <url-pattern>/details</url-pattern>
    </servlet-mapping>
              
 </web-app>